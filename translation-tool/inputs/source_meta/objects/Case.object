    <validationRules>
        <fullName>Appealed_Case_Cannot_be_cancelled</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(TEXT( HSvc_Escalation_Type__c))) &amp;&amp; OR(ISPICKVAL(Status, &apos;Cancelled&apos;), ISPICKVAL(Status, &apos;Associate&apos;)) &amp;&amp; (!(ISNULL(TEXT(Account.ShippingCountryCode))) &amp;&amp; CONTAINS(TEXT(Account.ShippingCountryCode), $Label.HSvc_ApplicableCountry))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Appealed case cannot be canceled or Associate</errorMessage>
    </validationRules>
	<validationRules>
        <fullName>Tier_2_CSR_change_Tier_1_Case_Reassign</fullName>
        <active>true</active>
        <errorConditionFormula>AND(HSvc_RecordType_Name__c = &apos;RepairWorkOrder&apos;, Account.HSvc_BusinessModel__c=&apos;ConsumerVAR&apos;, ISPICKVAL(Support_Tier__c,&apos;Tier 1&apos;),
		OR( $Profile.Name =&apos;INT CSR&apos;,
		$Profile.Name =&apos;INT CSM&apos;,
		$Profile.Name =&apos;Business Operations Admin&apos;),
		ISCHANGED(Case_Re_assign__c))</errorConditionFormula>
        <errorMessage>Case Reassign in Repair Work Order can be modified only by VAR.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSR_CSM_Cannot_Close_Case</fullName>
        <active>false</active>
        <description>CSR / CSM Cannot close the case before the Expected Resolved Date</description>
        <errorConditionFormula>($Profile.Name =  $Label.CSR_Profile || 
$Profile.Name = $Label.CSM_Profile ) &amp;&amp;
ISCHANGED(Status) &amp;&amp; ISPICKVAL(Status, &apos;Closed&apos;) &amp;&amp;
(
HSvc_Last_Date_for_Case_Resolution__c &gt;= NOW() || CONTAINS(TEXT(Hsvc_Problem_Category__c), &apos;Appeal&apos;)
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Case cannot be closed at this time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Create_NAP_Truck_Roll_on_B2C</fullName>
        <active>true</active>
        <description>Stop users from Creating Cases with Recommended Next Steps as NAP Truck Roll</description>
        <errorConditionFormula>AND(
	($RecordType.DeveloperName == $Label.Hsvc_B2CGeneralCaseRecType),
	IF(
	ISNEW(),
	ISPICKVAL(HSvc_RecommenedNextSteps__c,&apos;NAP Truck Roll&apos;),
	AND(ISCHANGED(HSvc_RecommenedNextSteps__c),ISPICKVAL(HSvc_RecommenedNextSteps__c,&apos;NAP Truck Roll&apos;))
	)
	)</errorConditionFormula>
        <errorDisplayField>HSvc_RecommenedNextSteps__c</errorDisplayField>
        <errorMessage>Cannot select Recommended Next Steps as NAP Truck Roll for a B2C Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_update_closed_cancelled_associate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.Name = &apos;B2C General Case&apos; , OR($Profile.Name = $Label.CSR_Profile, $Profile.Name = $Label.CSM_Profile), OR(ISPICKVAL( PRIORVALUE( Status), &apos;Closed&apos;),ISPICKVAL(PRIORVALUE(Status), &apos;Cancelled&apos;)), ISPICKVAL(PRIORVALUE(Status), &apos;Associate&apos;))</errorConditionFormula>
        <errorMessage>Cannot edit a Closed, Cancelled, or Associate Case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CaseSubjectOriginDescription_ReadOnly</fullName>
        <active>false</active>
        <description>Case Subject and Description should be Read Only once the case is created.Case Origin should also be Read Only once the case is created</description>
        <errorConditionFormula>IF(((ISCHANGED( Subject ))||(ISCHANGED( Origin ))||(ISCHANGED( Description))), True, false)</errorConditionFormula>
        <errorMessage>The following fields are not editable:  Case Origin, Case Subject and Description.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CaseVisibilityReadOnly_CSR</fullName>
        <active>false</active>
        <description>Case Visibility should be read only after case is created. CSR’s should not be able to but CSM’s should be able to edit the Case Visibility after the case is created</description>
        <errorConditionFormula>IF(( $Profile.Name == &apos;Hughes - CSR&apos;), ( IF(( ISCHANGED( Case_Internal__c ) ), true, false) ), false)</errorConditionFormula>
        <errorDisplayField>Case_Internal__c</errorDisplayField>
        <errorMessage>Do Not have permission to edit this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_Change_to_Appeal</fullName>
        <active>false</active>
        <description>Cannot change the case to Appeal if the petition or Queja is not resolved</description>
        <errorConditionFormula>(TEXT(Status) &lt;&gt; &apos;Resolved&apos;) &amp;&amp; 
(
(
CONTAINS(TEXT(PRIORVALUE(Hsvc_Problem_Category__c)), &apos;Claim&apos;) 
||
CONTAINS(TEXT(PRIORVALUE(Hsvc_Problem_Category__c)), &apos;Compliant&apos;)
)
&amp;&amp;
CONTAINS(TEXT(Hsvc_Problem_Category__c), &apos;Appeal&apos;)
)</errorConditionFormula>
        <errorDisplayField>Hsvc_Problem_Category__c</errorDisplayField>
        <errorMessage>Cannot Change the Problem Category if the case is not resolved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Escalation_Type</fullName>
