    <validationRules>
        <fullName>HSvc_RestrictChangeStatusAuthorisedUser</fullName>
        <active>true</active>
        <description>Only Authorized users can update the status as per case sub type and record type.</description>
        <errorConditionFormula>IF( ISPICKVAL(Case_Subtype__c, &apos;Regular&apos;) ,
IF( (ISPICKVAL( Status , &apos;MRB Approved&apos;) || ISPICKVAL( Status , &apos;Scheduled&apos;)) &amp;&amp; (ISCHANGED(Status) || ISNEW()) &amp;&amp; ($Profile.Name &lt;&gt; $Label.Hsvc_BkoutLink &amp;&amp; $Profile.Name &lt;&gt; $Label.HSvc_PSIntegration &amp;&amp; $Profile.Name &lt;&gt; $Label.Hsvc_SuperUserProfile) , true, false)
,
IF( ISPICKVAL(Case_Subtype__c, &apos;Emergency&apos;),
IF( (ISPICKVAL( Status , &apos;EMRB Approved&apos;) || ISPICKVAL( Status , &apos;Scheduled&apos;)) &amp;&amp; (ISCHANGED(Status) || ISNEW()) &amp;&amp; ($UserRole.DeveloperName &lt;&gt; $Label.HSvc_ChangeManager &amp;&amp; $Profile.Name &lt;&gt; $Label.HSvc_PSIntegration &amp;&amp; $Profile.Name &lt;&gt; $Label.Hsvc_SuperUserProfile), true, false)
,
IF( ISPICKVAL(Case_Subtype__c, &apos;Standard&apos;),
IF(ISPICKVAL( Status , &apos;Director Approved and Scheduled&apos;) &amp;&amp; (ISCHANGED(Status) || ISNEW()) &amp;&amp; ($Profile.Name &lt;&gt; $Label.Hsvc_BkoutLink &amp;&amp; $Profile.Name &lt;&gt; $Label.HSvc_PSIntegration &amp;&amp; $Profile.Name &lt;&gt; $Label.Hsvc_SuperUserProfile), true, false)
,
IF(RecordType.DeveloperName== $Label.HSvc_RecordtypeforRequest,
OR(AND( ISNEW() ,ISPICKVAL( Status , &apos;Build Out Released&apos;), ($Profile.Name &lt;&gt; $Label.Hsvc_BkoutLink &amp;&amp; $Profile.Name &lt;&gt; $Label.HSvc_PSIntegration &amp;&amp; $Profile.Name &lt;&gt; $Label.Hsvc_SuperUserProfile)),(AND( ISCHANGED(Status) ,ISPICKVAL( Status , &apos;Build Out Released&apos;), ($Profile.Name &lt;&gt; $Label.Hsvc_BkoutLink &amp;&amp; $Profile.Name &lt;&gt; $Label.HSvc_PSIntegration &amp;&amp; $Profile.Name &lt;&gt; $Label.Hsvc_SuperUserProfile) )))
,
false
)
)
)
)</errorConditionFormula>
        <errorDisplayField>HSvc_Status__c</errorDisplayField>
        <errorMessage>Only authorized users can set this status.</errorMessage>
    </validationRules>
