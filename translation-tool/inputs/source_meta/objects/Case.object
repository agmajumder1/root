        <fullName>Yahs_Case_Created_Date_in_UTC__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(MONTH(DATEVALUE(CreatedDate)))+&quot;/&quot; +TEXT(DAY(DATEVALUE(CreatedDate)))+&quot;/&quot; +TEXT(YEAR(DATEVALUE(CreatedDate))) &amp; &quot; &quot; &amp;
CASE(
MID(TEXT(CreatedDate), 12, 2),
&quot;01&quot;,&quot;1&quot;,&quot;02&quot;,&quot;2&quot;,&quot;03&quot;,&quot;3&quot;,&quot;04&quot;,&quot;4&quot;,&quot;05&quot;,&quot;5&quot;,&quot;06&quot;,&quot;6&quot;,&quot;07&quot;,&quot;7&quot;,&quot;08&quot;,&quot;8&quot;,&quot;09&quot;,&quot;9&quot;,&quot;10&quot;,&quot;10&quot;,
&quot;11&quot;,&quot;11&quot;,&quot;12&quot;,&quot;12&quot;,
&quot;13&quot;, &quot;1&quot;,
&quot;14&quot;, &quot;2&quot;,
&quot;15&quot;, &quot;3&quot;,
&quot;16&quot;, &quot;4&quot;,
&quot;17&quot;, &quot;5&quot;,
&quot;18&quot;, &quot;6&quot;,
&quot;19&quot;, &quot;7&quot;,
&quot;20&quot;, &quot;8&quot;,
&quot;21&quot;, &quot;9&quot;,
&quot;22&quot;, &quot;10&quot;,
&quot;23&quot;,&quot;11&quot;,
&quot;0&quot;
)
 &amp; &quot;:&quot; &amp;
MID(TEXT(CreatedDate), 15, 2) &amp; &quot; &quot; &amp;
IF(
VALUE( MID( TEXT( CreatedDate), 12, 2 ) ) &lt; 12,
&quot;AM&quot;,
&quot;PM&quot;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Case Created Date in UTC</label>
        <required>false</required>
