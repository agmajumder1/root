&amp; TEXT( FLOOR( MOD((NOW() -  LastModifiedDate) * 24 * 60, 60 ) )) &amp;  $Label.Hsvc_Minutes
&amp; TEXT(ROUND(MOD((NOW() -  LastModifiedDate) * 24 * 3600, 60),0)) &amp;   $Label.Hsvc_Seconds,
