    <validationRules>
        <fullName>Broadcast_Message_Cannot_Be_Blank</fullName>
        <active>true</active>
        <description>Validation rule to ensure English or Spanish Panama or Spanish Colombia or Spanish Peru or Spanish Ecuador or Spanish Chile or Spanish Argentina version of message not left blank</description>
        <errorConditionFormula>AND( 
LEN(Announcement_in_Panama__c)  = 0,
LEN( Announcement_esCO__c)  = 0,
LEN( Announcement_esPE__c)  = 0,
LEN( Announcement_esCL__c)  = 0,
LEN( Announcement_esEC__c)  = 0,
        LEN(Announcement_esAR__c)=0
    
)</errorConditionFormula>
        <errorMessage>You must enter an Announcement message and its Spanish version</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExpiryDateCannotBeSetupAsPastDate</fullName>
        <active>true</active>
        <description>This validation rule ensures that the expiry date is set in future and is greater that the effective date</description>
        <errorConditionFormula>OR(Expiry_Date__c &lt; NOW(),Expiry_Date__c &lt;Effective_Date__c)</errorConditionFormula>
        <errorDisplayField>Expiry_Date__c</errorDisplayField>
        <errorMessage>Expiry Date cannot be set in past and cannot be less than the effective date</errorMessage>
    </validationRules>
