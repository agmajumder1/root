    <validationRules>
        <fullName>HSvc_AlllowedDateDifference</fullName>
        <active>true</active>
        <description>Difference between start date and end date should not be</description>
        <errorConditionFormula>(HSvc_EndDate__c - HSvc_StartDate__c) &gt; 180</errorConditionFormula>
        <errorMessage>Difference between End Date and Start Date should be maximum of 180 days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_EndDategreaterthanStartDate</fullName>
        <active>true</active>
        <description>End date should be greater than start date</description>
        <errorConditionFormula>AND(DateCheck__c == true,OR(ISBLANK(HSvc_StartDate__c ) ,HSvc_StartDate__c &gt; HSvc_EndDate__c))</errorConditionFormula>
        <errorMessage>End Date should be greater than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_SeasonalSuspAllowedDateDifference</fullName>
        <active>true</active>
        <errorConditionFormula>((UPPER(Hsvc_TransactionCodeName__c) == &apos;LOCK&apos;) &amp;&amp; (UPPER(Hsvc_TransactionReasonName__c) == &apos;SEASONAL SUSPENSION&apos;)) &amp;&amp;
(((HSvc_EndDate__c - HSvc_StartDate__c) &gt; 120) || ((HSvc_EndDate__c - HSvc_StartDate__c) &lt; 30))</errorConditionFormula>
        <errorMessage>The duration of the suspension has to be greater than or equal to 30 days and less than or equal to 120 days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hsvc_ProcessDateCannotBeGreaterThanToday</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ProcessDate__c &lt; TODAY(),PRIORVALUE(ProcessDate__c) &lt;  ProcessDate__c)</errorConditionFormula>
        <errorDisplayField>ProcessDate__c</errorDisplayField>
        <errorMessage>Process date should be greater than today and less than current process date</errorMessage>
    </validationRules>
