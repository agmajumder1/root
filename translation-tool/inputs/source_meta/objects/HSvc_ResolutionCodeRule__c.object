    <fields>
        <fullName>Resolution_Description_Formula__c</fullName>
        <description>Description based on language</description>
        <externalId>false</externalId>
        <formula>CASE($Label.Current_Language_Key,&apos;es_CO&apos;, IF(ISBLANK( Description_Spanish__c ), Description__c ,Description_Spanish__c) ,&apos;es_PA&apos;, IF(ISBLANK( Hsvc_DescriptionPanama__c ), Description__c ,Hsvc_DescriptionPanama__c) ,&apos;es_PE&apos;, IF(ISBLANK( Hsvc_DescriptionPeru__c ), Description__c ,Hsvc_DescriptionPeru__c) ,&apos;es_EC&apos;, IF(ISBLANK( Hsvc_DescriptionEcuador__c ), Description__c ,Hsvc_DescriptionEcuador__c) ,&apos;es_CL&apos;, IF(ISBLANK( Hsvc_DescriptionChile__c ), Description__c ,Hsvc_DescriptionChile__c) ,&apos;es_AR&apos;, IF(ISBLANK( Hsvc_DescriptionArgentina__c ), Description__c ,Hsvc_DescriptionArgentina__c) , Description__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Description</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
