    <validationRules>
        <fullName>Device_ID_Suffix_Value_Validation</fullName>
        <active>true</active>
        <description>This Validation used to validate the Value of Device ID NOT to have NA, N/A, and NONE</description>
        <errorConditionFormula>IF( NOT ISNULL( Device_ID_Suffix__c ) &amp;&amp; ( UPPER(Device_ID_Suffix__c) =&apos;NA&apos;  || UPPER(Device_ID_Suffix__c) =&apos;N/A&apos; || UPPER(Device_ID_Suffix__c) =&apos;NONE&apos;), TRUE  , FALSE)</errorConditionFormula>
        <errorMessage>Device ID Suffix Should NOT be N/A or NA or NONE</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Code_Not_Editing_Once_Created</fullName>
        <active>true</active>
        <description>This Validation Rule is Used to Stop Editing the Product Code once it is Created</description>
        <errorConditionFormula>ISCHANGED( Product_Code__c )</errorConditionFormula>
        <errorDisplayField>Product_Code__c</errorDisplayField>
        <errorMessage>Product Code Cannot be Edited after it is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Code_and_Product_category_same</fullName>
        <active>false</active>
        <description>Validation rule to ensure that “Product Code” and “Product Category” having same value for Product type = Service or Support</description>
        <errorConditionFormula>IF( TEXT(Product_Type__c) = &apos;Service&apos; || TEXT(Product_Type__c) = &apos;Support&apos;, 
Product_Code__c  &lt;&gt;  TEXT(Product_Category__c), 
Product_Code__c  =  TEXT(Product_Category__c))</errorConditionFormula>
        <errorMessage>Product Code and Product Category should be the same value for Product type = Service or Support</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
