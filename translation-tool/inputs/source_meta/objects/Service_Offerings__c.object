    <validationRules>
        <fullName>IOES_Onetime_charge_validation_check</fullName>
        <active>true</active>
        <description>To verify onetime charge is not provided if Billing type is not One time charge</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Billing_Type__c,&apos;Recurring&apos;),ISPICKVAL(Billing_Type__c,&apos;Installment&apos;),ISPICKVAL(Billing_Type__c,&apos;Usage-Based&apos;)),
OR( Onetime_Charge__c  &lt;&gt; 0,NOT(ISNULL(Onetime_Charge__c))))</errorConditionFormula>
        <errorDisplayField>Onetime_Charge__c</errorDisplayField>
        <errorMessage>One time charge data is not valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IOES_RecurringCharge_Null_Check</fullName>
        <active>true</active>
        <description>To verify RecurringCharge is not provided if Billing type is not One time charge</description>
        <errorConditionFormula>AND(ISPICKVAL(Billing_Type__c,&apos;Onetime&apos;),
OR( Recurring_Charge__c  &lt;&gt; 0,NOT(ISNULL(Recurring_Charge__c))))</errorConditionFormula>
        <errorDisplayField>Recurring_Charge__c</errorDisplayField>
        <errorMessage>Recurring charge data is not valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IOES_Recurring_Charge_validation_check</fullName>
        <active>false</active>
        <description>To verify Recurring charge is provided for all SO&apos;s if billing type is recurring</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Billing_Type__c,&apos;Recurring&apos;),ISPICKVAL(Billing_Type__c,&apos;Installment&apos;),ISPICKVAL(Billing_Type__c,&apos;Usage-Based&apos;)),
ISNULL(Recurring_Charge__c) )</errorConditionFormula>
        <errorDisplayField>Recurring_Charge__c</errorDisplayField>
        <errorMessage>Recurring Charge is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IOES_Term_validation_check</fullName>
        <active>false</active>
        <description>To verify term is provided for all SO&apos;s</description>
        <errorConditionFormula>AND(Is_Active__c= TRUE ,OR(ISPICKVAL(Billing_Type__c,&apos;Recurring&apos;),
ISPICKVAL(Billing_Type__c,&apos;Installment&apos;),ISPICKVAL(Billing_Type__c,&apos;Usage-Based&apos;)),
Recurring_Charge__c    &lt;&gt;   0,
OR(Term__c=0,ISNULL(Term__c)))</errorConditionFormula>
        <errorDisplayField>Term__c</errorDisplayField>
        <errorMessage>Term is required</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
