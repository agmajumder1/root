        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Time_Remaining_Min_Sec__c</fullName>
        <externalId>false</externalId>
        <formula>if(
    NOT(Completion_Date__c==null),null,
    if(Target_Date__c&lt;now(),null,
       text(floor((Target_Date__c-now())*1440))
       +&apos;:&apos;+
       text(Mod(floor((Target_Date__c-now())*86400),60))))</formula>
        <label>Time Remaining (Min:Sec)</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Voilation__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(Now()&lt;Target_Date__c,Completion_Date__c&lt;Target_Date__c ),&apos;&apos;,
IMAGE(&quot;/img/permissions_deny16.gif&quot;, &apos;violated&apos;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Voilation</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
