<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>To keep the country wise mapping of google API attributes to address fields  of account.</description>
    <fields>
        <fullName>AttributeType__c</fullName>
        <description>This field will store the attribute type for which value will be picked from the Google API JSON</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>Please select the attribute type for which value should be mapped in Google API JSON.</inlineHelpText>
        <label>Attribute Type</label>
        <required>true</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>long_name</fullName>
                    <default>true</default>
                    <label>long_name</label>
                </value>
                <value>
                    <fullName>short_name</fullName>
                    <default>false</default>
                    <label>short_name</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>CountryCode__c</fullName>
        <description>This field will store the country code for which this mapping will be applicable.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>Please enter the country code for which this mapping is applicable.</inlineHelpText>
        <label>Country Code</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>FieldName__c</fullName>
        <description>This field will store the API name of the Account&apos;s address field.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>Please enter the API name of the address field on Account Object.</inlineHelpText>
        <label>Field Name</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>GoogleAPIAttributeName__c</fullName>
        <description>This field will store the attribute name from the Google API JSON.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Please enter the attribute name present in the Google API JSON.</inlineHelpText>
        <label>Google API Attribute Name</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Google API Address Field Mapping</label>
    <listViews>
        <fullName>All_Records</fullName>
        <columns>MasterLabel</columns>
        <columns>DeveloperName</columns>
        <columns>AttributeType__c</columns>
        <columns>CountryCode__c</columns>
        <columns>FieldName__c</columns>
        <columns>GoogleAPIAttributeName__c</columns>
        <filterScope>Everything</filterScope>
        <label>All Records</label>
        <language>en_US</language>
    </listViews>
    <pluralLabel>Google API Address Field Mappings</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
