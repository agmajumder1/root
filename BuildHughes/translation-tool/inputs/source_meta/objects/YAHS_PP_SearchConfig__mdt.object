        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Mark this true when you want the column to be visible on the search page results</inlineHelpText>
        <label>Display On UI</label>
        <type>Checkbox</type>
    </fields>
    <fields>
