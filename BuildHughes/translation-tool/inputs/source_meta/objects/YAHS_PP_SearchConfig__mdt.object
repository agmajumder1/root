        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Field Type</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value>
                    <fullName>Date</fullName>
                    <default>true</default>
                    <label>Date</label>
                </value>
                <value>
                    <fullName>Lookup</fullName>
                    <default>false</default>
                    <label>Lookup</label>
                </value>
                <value>
                    <fullName>Picklist</fullName>
                    <default>false</default>
                    <label>Picklist</label>
                </value>
                <value>
                    <fullName>Text</fullName>
                    <default>false</default>
                    <label>Text</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>LookupPage__c</fullName>
        <description>When the Field Type is Lookup, choose the name of the page to redirect to when the lookup icon is clicked on the search page</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>When the Field Type is Lookup, choose the name of the page to redirect to when the lookup icon is clicked on the search page</inlineHelpText>
        <label>LookupPage</label>
        <length>255</length>
