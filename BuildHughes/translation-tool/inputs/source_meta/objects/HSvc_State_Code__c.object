    <fields>
        <fullName>Product_Code__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>Look up to Product Code field on HNS Product object filtered by Product Type ‘Service’</description>
        <externalId>false</externalId>
        <label>Product</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Only Products of Type Service can be selected</errorMessage>
            <filterItems>
                <field>HNS_Products__c.Product_Type__c</field>
                <operation>equals</operation>
                <value>Service</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>HNS_Products__c</referenceTo>
        <relationshipLabel>State Codes</relationshipLabel>
        <relationshipName>Product_Codes</relationshipName>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
