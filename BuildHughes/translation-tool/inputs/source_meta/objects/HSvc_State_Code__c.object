    </fields>
    <fields>
        <fullName>State_Code_Description__c</fullName>
        <description>This is the formula field for multiple language which will be referenced in Classes and will be displayed to user.</description>
        <externalId>false</externalId>
        <formula>CASE($Label.Current_Language_Key,&apos;es_CO&apos;, IF(ISBLANK(State_Code_Description_ESCO__c), State_Code_Description_ENUS__c,State_Code_Description_ESCO__c), &apos;es_PA&apos;, IF(ISBLANK(Hsvc_StateCodeDescriptionPanama__c), State_Code_Description_ENUS__c,Hsvc_StateCodeDescriptionPanama__c),&apos;es_PE&apos;, IF(ISBLANK(Hsvc_StateCodeDescriptionPeru__c), State_Code_Description_ENUS__c,Hsvc_StateCodeDescriptionPeru__c),&apos;es_EC&apos;, IF(ISBLANK(Hsvc_StateCodeDescriptionEcuador__c), State_Code_Description_ENUS__c,Hsvc_StateCodeDescriptionEcuador__c),&apos;es_CL&apos;, IF(ISBLANK(Hsvc_StateCodeDescriptionChile__c), State_Code_Description_ENUS__c,Hsvc_StateCodeDescriptionChile__c),&apos;es_AR&apos;, IF(ISBLANK(Hsvc_StateCodeDescriptionArgentina__c), State_Code_Description_ENUS__c,Hsvc_StateCodeDescriptionArgentina__c), State_Code_Description_ENUS__c)</formula>
        <label>State Code Description</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>is_Active__c</fullName>
        <description>To find out whether or not a State Code is Active</description>
        <externalId>false</externalId>
        <formula>IF( AND( Effective_Date__c &lt;= TODAY() , Expiry_Date__c &gt; TODAY()),True, False)</formula>
        <label>Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
