    <fields>
        <fullName>State_Code_Description_ESCO__c</fullName>
        <description>This is state code description field for Spanish Language.</description>
        <externalId>false</externalId>
        <label>State Code Description(Colombia)</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
