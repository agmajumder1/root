    <validationRules>
        <fullName>ValidatingCustomerProgram_Status</fullName>
        <active>true</active>
        <description>SFDC shall have a validation to not allow change of Customer Program&apos;s status if it is set to active once.</description>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(Status__c),&quot;Active&quot;),NOT(ISNEW()),
 ISCHANGED(Status__c) )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status cannot be changed if it is made as Active</errorMessage>
    </validationRules>
