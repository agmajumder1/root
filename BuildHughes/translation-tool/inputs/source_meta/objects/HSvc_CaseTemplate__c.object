    <validationRules>
        <fullName>ExpiryDateCannotBeSetUpInPast</fullName>
        <active>true</active>
        <description>This validation rule ensures that the expiry date is set in future and is greater that the effective date</description>
        <errorConditionFormula>OR(ExpiryDate__c &lt; TODAY(),ExpiryDate__c &lt; EffectiveDate__c)</errorConditionFormula>
        <errorDisplayField>ExpiryDate__c</errorDisplayField>
        <errorMessage>Expiry Date cannot be set in past and cannot be less than the effective date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_CreateChildCase</fullName>
        <active>true</active>
        <description>Rules to make Child Case Quick Case Create template required if Child Case checkbox is checked</description>
        <errorConditionFormula>OR(
AND(HSvc_Create_Child_Case_s__c == True &amp;&amp; ISBLANK(HSvc_ChildCaseQuickCaseCreateTempla__c)),
AND(HSvc_Create_Child_Case_s__c == False &amp;&amp; NOT(ISBLANK(HSvc_ChildCaseQuickCaseCreateTempla__c)))
)</errorConditionFormula>
        <errorDisplayField>HSvc_ChildCaseQuickCaseCreateTempla__c</errorDisplayField>
        <errorMessage>Plase select child case template</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_DisableOpeninEditMode</fullName>
        <active>true</active>
        <description>When &quot;Available to Profile&quot; is selected as &quot;System&quot; or &quot;Customer Portal Defaults&quot;, then the &quot;Open in Edit Mode&quot; check box cannot be checked</description>
        <errorConditionFormula>OR(ISPICKVAL(HSvc_Available_to_Profile__c,&apos;System&apos;),ISPICKVAL(HSvc_Available_to_Profile__c,&apos;Customer Portal Defaults&apos;)) &amp;&amp; Open_in_Edit_Mode__c == true</errorConditionFormula>
        <errorMessage>Open in Edit Mode cannot be selected, if Available to Profile is set to System or Customer Portal Defaults</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_MinutesUntilFollowUpDue</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL( HSvc_Case_Record_type__c , &apos;B2B General Case&apos;) &amp;&amp; ISPICKVAL(Hsvc_Support_Tier__c,&apos;Consumer Tier 1&apos;) &amp;&amp; NOT(ISPICKVAL( HSvc_Case_Status__c , &apos;Closed&apos;)) &amp;&amp; ISBLANK(FollowUpDueDate__c))
 || ( (ISPICKVAL( HSvc_Case_Record_type__c , &apos;B2C General Case&apos;) &amp;&amp; (( ISPICKVAL( HSvc_Case_Status__c , &apos;Open - Escalated&apos;)
 || ISPICKVAL( HSvc_Case_Status__c , &apos;Pending - Callback&apos;) || ISPICKVAL( HSvc_Case_Status__c , &apos;Pending - Customer Confirmation&apos;) 
 || ISPICKVAL( HSvc_Case_Status__c , &apos;Pending - Customer Input&apos;))) &amp;&amp; ISBLANK(FollowUpDueDate__c)))</errorConditionFormula>
        <errorDisplayField>FollowUpDueDate__c</errorDisplayField>
        <errorMessage>Field Cannot be left blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_PendingReactivationStatus</fullName>
        <active>true</active>
        <description>Is status is Pending Reactivation, user should not be able to select that template</description>
        <errorConditionFormula>ISPICKVAL( HSvc_Case_Status__c,&apos;Pending Re-Activation&apos;)  &amp;&amp; NOT( ISPICKVAL( HSvc_Available_to_Profile__c,&apos;System&apos;))</errorConditionFormula>
        <errorDisplayField>HSvc_Available_to_Profile__c</errorDisplayField>
        <errorMessage>Template with Case status &apos;Pending Re-Activation&apos; are available for System Only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_RecommentNextStepSelection</fullName>
        <active>true</active>
        <description>When &quot;Available to Profile&quot; is selected as &quot;User&quot;, then the use cannot save the template with the following Recommended Next Steps picklist values: Hughes Truck Roll, NAP Truck Roll, Vendor Meet, and NAP Office Only</description>
        <errorConditionFormula>ISPICKVAL(HSvc_Available_to_Profile__c,&apos;User&apos;) &amp;&amp;
(
ISPICKVAL(HSvc_Recommended_Next_Step__c,&apos;Hughes Truck Roll&apos;) ||
ISPICKVAL(HSvc_Recommended_Next_Step__c,&apos;NAP Truck Roll&apos;) ||
ISPICKVAL(HSvc_Recommended_Next_Step__c,&apos;Vendor Meet&apos;) ||
ISPICKVAL(HSvc_Recommended_Next_Step__c,&apos;NAP Office Only&apos;)
)</errorConditionFormula>
        <errorMessage>Recommended Next Step cannot be set to either of Hughes Truck Roll, NAP Truck Roll, NAP Office Only and Vendor Meet when template is marked available to user</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HughesSvc_CaseResolutiononCaseTemplate</fullName>
        <active>true</active>
        <description>This validation ensures that Case resolution and Case Resolution must be there while closing a case</description>
        <errorConditionFormula>AND(
			ISPICKVAL(HSvc_Case_Status__c,&apos;Closed&apos;),
			OR(
				ISBLANK(HSvc_Resolution__c),
				ISBLANK(HSVC_Resolution_Code_Rule__c)
			),
			OR(
				$RecordType.DeveloperName == $Label.Hsvc_B2BGeneralCaseRecType,
				$RecordType.DeveloperName == $Label.Hsvc_B2CGeneralCaseRecType,
				$RecordType.DeveloperName == $Label.Hsvc_InstallWorkOrdersRecType,
				$RecordType.DeveloperName == $Label.Hsvc_OfficeOnlyRecType,
				$RecordType.DeveloperName == $Label.Hsvc_RepairWorkOrderRecType,
				$RecordType.DeveloperName == $Label.Hsvc_RMARecType
			)
		)</errorConditionFormula>
        <errorMessage>Cannot close case without Resolution and Resolution Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecommendedNextStepDesc_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISBLANK(TEXT(HSvc_Recommended_Next_Step__c) )),
OR(ISNULL( HSvc_Recommended_Next_Steps__c),
ISBLANK(HSvc_Recommended_Next_Steps__c))
)</errorConditionFormula>
        <errorDisplayField>HSvc_Recommended_Next_Steps__c</errorDisplayField>
        <errorMessage>Cannot leave this field blank if Recommended Next step is selected</errorMessage>
    </validationRules>
