    <fields>
        <fullName>KB_Exit_Code_Description__c</fullName>
        <externalId>false</externalId>
        <formula>CASE($Label.Current_Language_Key,&apos;es_CO&apos;, IF(ISBLANK(KB_Exit_Code_Description_esCO__c), KB_Exit_Code_Description_enUS__c,KB_Exit_Code_Description_esCO__c), &apos;es_PA&apos;, IF(ISBLANK(Hsvc_KBExitCodeDescriptionPanama__c), KB_Exit_Code_Description_enUS__c,Hsvc_KBExitCodeDescriptionPanama__c),&apos;es_AR&apos;, IF(ISBLANK(Hsvc_KBExitCodeDescriptionArgentina__c), KB_Exit_Code_Description_enUS__c,Hsvc_KBExitCodeDescriptionArgentina__c), KB_Exit_Code_Description_enUS__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>KB Exit Code Description</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
