    <fields>
        <fullName>is_Active__c</fullName>
        <description>To find out whether or not a KB Exit Code is Active</description>
        <externalId>false</externalId>
        <formula>IF( AND( Effective_Date__c &lt;= TODAY(), Expiry_Date__c &gt; TODAY()),True, False)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
