    <validationRules>
        <fullName>ExitCodeDescriptionNotBlank</fullName>
        <active>true</active>
        <description>This validation checks that all of the description field should not be empty.</description>
        <errorConditionFormula>((KB_Exit_Code_Description_esCO__c ==NULL ||KB_Exit_Code_Description_esCO__c ==&apos;&apos;) &amp;&amp;  (KB_Exit_Code_Description_enUS__c ==NULL || KB_Exit_Code_Description_enUS__c ==&apos;&apos;))</errorConditionFormula>
        <errorMessage>Please enter value in atleast One description field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExpiryDateCannotBeSetUpInPast</fullName>
        <active>true</active>
        <description>This validation rule ensures that the expiry date is set in future and is greater that the effective date</description>
        <errorConditionFormula>OR(Expiry_Date__c &lt; TODAY(),Expiry_Date__c &lt; Effective_Date__c)</errorConditionFormula>
        <errorDisplayField>Expiry_Date__c</errorDisplayField>
        <errorMessage>Expiry Date cannot be set in past and cannot be less than the effective date.</errorMessage>
    </validationRules>
