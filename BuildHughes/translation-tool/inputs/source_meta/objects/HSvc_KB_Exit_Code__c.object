        <fullName>KB_Exit_Code_Description_esCO__c</fullName>
        <description>Description of KB Exit Code.
Ex: Change of Account Ownership, Complimentary Restore Tokens, etc.</description>
        <externalId>false</externalId>
        <label>KB Exit Code Description_esCO</label>
        <length>255</length>
        <required>false</required>
