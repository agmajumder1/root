        <formula>CASE($Label.Current_Language_Key,&apos;es_CO&apos;, IF(ISBLANK( HSvc_Reason_Description_Spanish__c ), HSvc_Reason_Description__c ,HSvc_Reason_Description_Spanish__c) ,&apos;es_PA&apos;, IF(ISBLANK( Hsvc_ReasonDescriptionPanama__c ), HSvc_Reason_Description__c ,Hsvc_ReasonDescriptionPanama__c) , &apos;es_PE&apos;, IF(ISBLANK( Hsvc_ReasonDescriptionPeru__c ), HSvc_Reason_Description__c ,Hsvc_ReasonDescriptionPeru__c) , &apos;es_CL&apos;, IF(ISBLANK(Hsvc_ReasonDescriptionChile__c), HSvc_Reason_Description__c ,Hsvc_ReasonDescriptionChile__c) , &apos;es_EC&apos;, IF(ISBLANK( Hsvc_ReasonDescriptionEcuador__c), HSvc_Reason_Description__c ,Hsvc_ReasonDescriptionEcuador__c) , &apos;es_AR&apos;, IF(ISBLANK( Hsvc_ReasonDescriptionArgentina__c), HSvc_Reason_Description__c ,Hsvc_ReasonDescriptionArgentina__c),HSvc_Reason_Description__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
